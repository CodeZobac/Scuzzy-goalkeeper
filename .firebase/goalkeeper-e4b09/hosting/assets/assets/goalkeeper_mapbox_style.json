{
  "version": 8,
  "name": "Goalkeeper Green Theme",
  "metadata": {
    "mapbox:autocomposite": false,
    "mapbox:type": "template",
    "mapbox:sdk-support": {
      "js": "0.45.0",
      "android": "6.0.0",
      "ios": "4.0.0"
    }
  },
  "sources": {
    "mapbox-streets": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v8"
    }
  },
  "sprite": "mapbox://sprites/mapbox/streets-v11",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#00A85A"
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", "class", "park"],
      "paint": {
        "fill-color": "#0BB566",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "water",
      "paint": {
        "fill-color": "#008f4c"
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "building",
      "paint": {
        "fill-color": "#0BB566",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["in", "class", "street", "street_limited"],
      "paint": {
        "line-color": "#008f4c",
        "line-width": {
          "base": 1.5,
          "stops": [
            [12, 0.5],
            [14, 2],
            [18, 18]
          ]
        }
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["in", "class", "primary"],
      "paint": {
        "line-color": "#007a40",
        "line-width": {
          "base": 1.5,
          "stops": [
            [5, 0.75],
            [18, 32]
          ]
        }
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["in", "class", "secondary", "tertiary"],
      "paint": {
        "line-color": "#008f4c",
        "line-width": {
          "base": 1.5,
          "stops": [
            [8, 0.5],
            [18, 20]
          ]
        }
      }
    },
    {
      "id": "road-trunk",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["==", "class", "trunk"],
      "paint": {
        "line-color": "#007a40",
        "line-width": {
          "base": 1.5,
          "stops": [
            [5, 0.75],
            [18, 32]
          ]
        }
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["==", "class", "motorway"],
      "paint": {
        "line-color": "#007a40",
        "line-width": {
          "base": 1.5,
          "stops": [
            [5, 0.75],
            [18, 32]
          ]
        }
      }
    },
    {
      "id": "admin-0-boundary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "admin",
      "filter": ["<=", "admin_level", 2],
      "paint": {
        "line-color": "#146b3a",
        "line-width": {
          "base": 1.3,
          "stops": [
            [3, 0.5],
            [22, 15]
          ]
        }
      }
    },
    {
      "id": "admin-1-boundary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "admin",
      "filter": ["==", "admin_level", 3],
      "paint": {
        "line-color": "#146b3a",
        "line-width": {
          "base": 1.3,
          "stops": [
            [4, 0.4],
            [22, 8]
          ]
        }
      }
    },
    {
      "id": "place-city-large",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["all", ["==", "type", "city"], [">", "scalerank", 2]],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Bold", "Arial Unicode MS Bold"],
        "text-size": {
          "base": 1.2,
          "stops": [
            [7, 14],
            [11, 24]
          ]
        }
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#000000",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-city-medium",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["all", ["==", "type", "city"], ["<=", "scalerank", 2]],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Bold", "Arial Unicode MS Bold"],
        "text-size": {
          "base": 1.2,
          "stops": [
            [6, 12],
            [10, 16]
          ]
        }
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#000000",
        "text-halo-width": 2
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "road_label",
      "filter": [
        "in",
        "class",
        "primary",
        "secondary",
        "tertiary",
        "trunk",
        "motorway"
      ],
      "layout": {
        "text-field": "{name}",
        "text-font": ["Open Sans Regular", "Arial Unicode MS Regular"],
        "text-size": 12,
        "symbol-placement": "line"
      },
      "paint": {
        "text-color": "#ffffff",
        "text-halo-color": "#000000",
        "text-halo-width": 2
      }
    }
  ]
}
